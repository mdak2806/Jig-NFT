<h1>Search Results</h1>
<p>Find me in app/views/search/index.html.erb</p>

<div class="work-index">
<%  @works.each do |work| %> 
<li>
    <a><%= link_to work.name, work_path(work.id) %></a>
    <br>
     <%= work.artist %>
     <br>
    <%= image_tag work.picture, alt:work.name, class:'NFT' %>
  </li>
<% end %>
</div>


